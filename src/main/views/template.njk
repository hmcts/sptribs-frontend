{% extends "govuk/template.njk" %}

{% block head %}
  {% include "webpack/css.njk" %}
{% endblock %}

{% block bodyEnd %}
  {# Run JavaScript at end of the <body>, to avoid blocking the initial render. #}
  {% include "webpack/js.njk" %}
{% endblock %}

{% block header %}
    { { govukHeader({serviceName: content.serviceTitle}) } }
{% endblock %}

{% block beforeContent %}
{{ govukPhaseBanner({
  tag: {
    text: "BETA"
  },
  html: content.banner|replace("currentUrl",currentUrl)
}) }}
{% endblock %}
